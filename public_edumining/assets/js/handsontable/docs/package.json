{
  "name": "handsontable-documentation",
  "private": true,
  "version": "0.0.0",
  "description": "Handsontable documentation",
  "main": "index.js",
  "author": "Handsoncode <hello@handsontable.com>",
  "repository": {
    "url": "https://github.com/handsontable/handsontable",
    "directory": "docs"
  },
  "scripts": {
    "docs:lint": "eslint --ext .js,.vue .vuepress next",
    "docs:lint:fix": "eslint --ext .js,.vue .vuepress next --fix",
    "docs:assets:next": "node .vuepress/tools/link-assets.mjs",
    "docs:start": "npm run docs:assets:next && vuepress dev ./",
    "docs:start:no-cache": "npm run docs:assets:next && vuepress dev --no-cache ./",
    "docs:build": "npm run docs:build:no-check-links && npm run docs:check-links",
    "docs:build:no-check-links": "vuepress build -d .vuepress/dist/docs ./",
    "docs:docker:build": "npm run docs:docker:build:staging",
    "docs:docker:build:staging": "npm run docs:assets:next && docker build -f docker/Dockerfile -t docs-md ../",
    "docs:docker:build:production": "docker build -f docker/Dockerfile.production -t docs-md:production ../",
    "docs:version": "cd .vuepress/tools/version && node rollup-a-version.js",
    "docs:api": "cd .vuepress/tools/jsdoc-convert && node jsdoc.js",
    "docs:check-links": "node .vuepress/tools/check-links.js"
  },
  "license": "CC BY 4.0",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@vuepress/plugin-active-header-links": "^1.8.2",
    "@vuepress/plugin-search": "^1.8.2",
    "@vuepress/shared-utils": "^1.8.2",
    "babel-eslint": "^10.1.0",
    "broken-link-checker": "^0.7.8",
    "chalk": "^4.1.1",
    "copy-webpack-plugin": "^5.1.2",
    "dmd": "^6.0.0",
    "fs-extra": "^9.1.0",
    "inquirer": "^8.1.1",
    "jsdoc-to-markdown": "^7.0.1",
    "replace-in-files": "^3.0.0",
    "semver": "^5.7.1",
    "vue-eslint-parser": "^7.6.0",
    "vue-router": "^3.5.1",
    "vue-tabs-component": "^1.5.0",
    "vuepress": "^1.8.2",
    "vuepress-plugin-sitemap": "^2.3.1",
    "vuepress-plugin-tabs": "^0.3.0",
    "xlsx-renderer": "^2.4.0"
  }
}
